@model IEnumerable<HajHakathon.Models.G_Users>


@{
    ViewBag.Title = "Index";
}




<section id="configuration">
    <div class="row">
        <div class="col-12">
            <div class="card">
                <div class="card-header">
                    <h4 class="card-title">@ViewBag.Title</h4>
                    <a class="heading-elements-toggle"><i class="la la-ellipsis-v font-medium-3"></i></a>
                    <div class="heading-elements">
                        <ul class="list-inline mb-0">
                            <li><a data-action="collapse"><i class="ft-minus"></i></a></li>
                            <li><a data-action="reload"><i class="ft-rotate-cw"></i></a></li>
                            <li><a data-action="expand"><i class="ft-maximize"></i></a></li>
                            <li><a data-action="close"><i class="ft-x"></i></a></li>
                        </ul>
                    </div>
                </div>
                <div class="card-content collapse show">
                    <div class="card-body card-dashboard">
                        <a href="@Url.Action("Create")" class="btn btn-primary btn-min-width box-shadow-1 mr-1 mb-1" role="button" aria-pressed="true"><i class="la la-plus"></i>@HajHakathon.Resources.GbResoures.NewRecored</a>
                        <table class="table table-striped table-bordered dataex-res-configuration">
                            <thead>
                                <tr>
		<th>@Html.DisplayNameFor(model => model.NameAr)</th>
		<th>@Html.DisplayNameFor(model => model.NameEn)</th>
		<th>@Html.DisplayNameFor(model => model.PassportID)</th>
		<th>@Html.DisplayNameFor(model => model.BirthDate)</th>
		<th>@Html.DisplayNameFor(model => model.VisaNumber)</th>
		<th>@Html.DisplayNameFor(model => model.BarcodeIQ)</th>
		<th>@Html.DisplayNameFor(model => model.AirLinesCompanyID)</th>
		<th>@Html.DisplayNameFor(model => model.TawafaCompanyID)</th>
		<th>@Html.DisplayNameFor(model => model.Gender)</th>
		<th>@Html.DisplayNameFor(model => model.Photo)</th>
		<th>@Html.DisplayNameFor(model => model.TouchID)</th>
		<th>@Html.DisplayNameFor(model => model.PassportSource)</th>
		<th>@Html.DisplayNameFor(model => model.LanguageID)</th>
		<th>@Html.DisplayNameFor(model => model.PortNumber)</th>
		<th>@Html.DisplayNameFor(model => model.VisaIssuance)</th>
		<th>@Html.DisplayNameFor(model => model.IsNedded)</th>
		<th>@Html.DisplayNameFor(model => model.BlodGroup)</th>
		<th>@Html.DisplayNameFor(model => model.MobileNumber)</th>
		<th>@Html.DisplayNameFor(model => model.IdentityNumber)</th>
		<th>@Html.DisplayNameFor(model => model.TasreeeNumber)</th>
		<th>@Html.DisplayNameFor(model => model.Email)</th>
		<th>@Html.DisplayNameFor(model => model.UserName)</th>
		<th>@Html.DisplayNameFor(model => model.Password)</th>
		<th>@Html.DisplayNameFor(model => model.Sys_Countaries.NameAr)</th>
		<th>@Html.DisplayNameFor(model => model.Sys_Languages.NameAr)</th>
        <th>@HajHakathon.Resources.GbResoures.tableOption</th>
    </tr>
   </thead>
    <tbody>
	
@foreach (var item in Model) {
    <tr>
	

		<td>@Html.DisplayFor(modelItem => item.NameAr)</td>

		<td>@Html.DisplayFor(modelItem => item.NameEn)</td>

		<td>@Html.DisplayFor(modelItem => item.PassportID)</td>

		<td>@Html.DisplayFor(modelItem => item.BirthDate)</td>

		<td>@Html.DisplayFor(modelItem => item.VisaNumber)</td>

		<td>@Html.DisplayFor(modelItem => item.BarcodeIQ)</td>

		<td>@Html.DisplayFor(modelItem => item.AirLinesCompanyID)</td>

		<td>@Html.DisplayFor(modelItem => item.TawafaCompanyID)</td>

		<td>@Html.DisplayFor(modelItem => item.Gender)</td>

		<td>@Html.DisplayFor(modelItem => item.Photo)</td>

		<td>@Html.DisplayFor(modelItem => item.TouchID)</td>

		<td>@Html.DisplayFor(modelItem => item.PassportSource)</td>

		<td>@Html.DisplayFor(modelItem => item.LanguageID)</td>

		<td>@Html.DisplayFor(modelItem => item.PortNumber)</td>

		<td>@Html.DisplayFor(modelItem => item.VisaIssuance)</td>

		<td>@Html.DisplayFor(modelItem => item.IsNedded)</td>

		<td>@Html.DisplayFor(modelItem => item.BlodGroup)</td>

		<td>@Html.DisplayFor(modelItem => item.MobileNumber)</td>

		<td>@Html.DisplayFor(modelItem => item.IdentityNumber)</td>

		<td>@Html.DisplayFor(modelItem => item.TasreeeNumber)</td>

		<td>@Html.DisplayFor(modelItem => item.Email)</td>

		<td>@Html.DisplayFor(modelItem => item.UserName)</td>

		<td>@Html.DisplayFor(modelItem => item.Password)</td>

		<td>@Html.DisplayFor(modelItem => item.Sys_Countaries.NameAr)</td>

		<td>@Html.DisplayFor(modelItem => item.Sys_Languages.NameAr)</td>

        
		<td>
		<div class="btn-group btn-group-sm">
		<a href="@Url.Action("Edit",new { id=item.ID})" class="btn btn-warning btn-min-width box-shadow-4 mr-1 mb-1" role="button" aria-pressed="true"><i class="la la-plus"></i>@HajHakathon.Resources.GbResoures.Edit</a>
		</div>
		</td>
    </tr>
}
	 </tbody>
                        </table>
                    </div>
                </div>
            </div>
        </div>
    </div>
</section>




	@section ScriptsAfterVendor{
